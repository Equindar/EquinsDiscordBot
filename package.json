{
  "name": "equinsdiscordbot",
  "version": "1.0.0",
  "description": "A discord bot, written in typescript",
  "main": "index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node build/index.js",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write .",
    "format:check": "prettier --cache --check .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Equindar/EquinsDiscordBot.git"
  },
  "author": "Equindar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Equindar/EquinsDiscordBot/issues"
  },
  "homepage": "https://github.com/Equindar/EquinsDiscordBot#readme",
  "dependencies": {
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "prettier": "^3.3.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}